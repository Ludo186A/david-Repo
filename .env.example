# Database Configuration (External SSD Supabase)
DATABASE_URL=postgresql://postgres:postgres@localhost:54334/postgres
SUPABASE_BASE_PATH=/Volumes/Extreme SSD/ict_project_2/supabase

# LLM Provider Configuration
LLM_PROVIDER=ollama

# Ollama Configuration (from INITIALOLLAMA.MD)
OLLAMA_BASE_PATH=/Volumes/Extreme SSD/ollama
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama3.2
OLLAMA_EMBEDDING_MODEL=nomic-embed-text
OLLAMA_TIMEOUT=30

# OpenAI Configuration (fallback)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o
OPENAI_BASE_URL=https://api.openai.com/v1

# Anthropic Configuration (fallback)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# RAG Configuration (enhanced for Ollama integration)
RAG_EMBEDDING_PROVIDER=ollama
RAG_EMBEDDING_MODEL=nomic-embed-text
RAG_VECTOR_DIMENSIONS=768

# Performance Configuration (from INITIALOLLAMA.MD)
MAX_RETRIES=3
HEALTH_CHECK_INTERVAL=60
PERFORMANCE_THRESHOLD=5.0
ENABLE_QUERY_CACHING=true
CACHE_TTL_SECONDS=3600
MAX_CONCURRENT_QUERIES=10
